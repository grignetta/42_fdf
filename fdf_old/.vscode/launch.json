{
	"configurations": [
	{
		"name": "Debug",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/fdf",
		"args": ["42.fdf"],
		"stopAtEntry": true,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
			{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			},
			{
				"description": "Set Disassembly Flavor to Intel",
				"text": "-gdb-set disassembly-flavor intel",
				"ignoreFailures": true
			}
		]
	}
	]
}